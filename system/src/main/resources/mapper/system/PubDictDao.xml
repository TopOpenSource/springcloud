<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.PubDictDao">
	
	<resultMap id="PubDictMap" type="com.sdstc.system.model.PubDict">
	    <id column="id" property="id" />
	    <result column="name" property="name" />
	    <result column="code" property="code" />
	    <result column="p_id" property="pId" />
	    <result column="path" property="path" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	</resultMap>
	
    <sql id="PubDictCols">
      id,name,code,p_id,path,gmt_create,gmt_modified,create_account,modified_account
    </sql>
    
    <sql id="PubDictProps">
      #{id},#{name},#{code},#{pId},#{path},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount}
    </sql>
    
    <sql id="PubDictWhere">
      
    </sql>
    
    <sql id="PubDictDtoWhere">
      
    </sql>
    
	
	<select id="selectByDto"  parameterType="com.sdstc.system.model.PubDict" resultMap="PubDictMap">
		select
			<include refid="PubDictCols"></include>
		from pub_dict
		<where>
		    <include refid="PubDictWhere"></include>
		</where>
	</select>
	
	<select id="selectCountByDto"  parameterType="com.sdstc.system.model.PubDict" resultType="Integer">
		select
			count(*)
		from pub_dict
		<where>
		    <include refid="PubDictWhere"></include>
		</where>
	</select>
	
	<select id="selectPageByDto" resultMap="PubDictMap">
		select
			<include refid="PubDictCols"></include>
		from pub_file
		<where>
		    <include refid="PubDictDtoWhere"></include>
		</where>
		limit #{pageDto.start},#{pageDto.pageSize}
	</select>
</mapper>