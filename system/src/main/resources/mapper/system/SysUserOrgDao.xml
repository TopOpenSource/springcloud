<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.SysUserOrgDao">
	
	<resultMap id="SysUserOrgMap" type="com.sdstc.system.model.SysUserOrg">
	    <id column="id" property="id" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="modified_account" property="modifiedAccount" />
	    <result column="create_account" property="createAccount" />
	    <result column="customer_id" property="customerId" />
	    <result column="is_delete" property="isDelete" />
	    <result column="org_id" property="orgId" />
	    <result column="user_account" property="userAccount" />
	    <result column="state" property="state" />
	</resultMap>
	
    <sql id="SysUserOrgCols">
      id,gmt_create,gmt_modified,modified_account,create_account,customer_id,is_delete,org_id,user_account,state
    </sql>
    
    <sql id="SysUserOrgProps">
      #{id},#{gmtCreate},#{gmtModified},#{modifiedAccount},#{createAccount},#{customerId},#{isDelete},#{orgId},#{userAccount},#{state}
    </sql>
    
    <sql id="SysUserOrgWhere">
      
    </sql>
    
    <sql id="SysUserOrgDtoWhere">
      
    </sql>
	
	<select id="selectByDto"  parameterType="com.sdstc.system.model.SysUserOrg" resultMap="SysUserOrgMap">
		select
			<include refid="SysUserOrgCols"></include>
		from sys_user_org
		<where>
		    <include refid="SysUserOrgWhere"></include>
		</where>
	</select>
	
	<select id="selectCountByDto"  parameterType="com.sdstc.system.model.SysUserOrg" resultType="Integer">
		select
			count(*)
		from sys_user_org
		<where>
		    <include refid="SysUserOrgWhere"></include>
		</where>
	</select>
	
	<select id="selectPageByDto" resultMap="SysUserOrgMap">
		select
			<include refid="SysUserOrgCols"></include>
		from sys_user_org
		<where>
		    <include refid="SysUserOrgDtoWhere"></include>
		</where>
		limit #{pageDto.start},#{pageDto.pageSize}
	</select>
</mapper>