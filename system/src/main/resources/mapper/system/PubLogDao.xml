<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.PubLogDao">
	
	<resultMap id="PubLogMap" type="com.sdstc.system.model.PubLog">
	    <id column="id" property="id" />
	    <result column="customer_id" property="customerId" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	    <result column="type" property="type" />
	    <result column="level" property="level" />
	    <result column="info" property="info" />
	    <result column="status" property="status" />
	</resultMap>
	
    <sql id="PubLogCols">
      id,customer_id,gmt_create,gmt_modified,create_account,modified_account,type,level,info,status
    </sql>
    
    <sql id="PubLogProps">
      #{id},#{customerId},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount},#{type},#{level},#{info},#{status}
    </sql>
    
    <sql id="PubLogWhere">
      
    </sql>
    
    <sql id="PubLogDtoWhere">
      
    </sql>
    
	
	<select id="selectByDto"  parameterType="com.sdstc.system.model.PubLog" resultMap="PubLogMap">
		select
			<include refid="PubLogCols"></include>
		from pub_log
		<where>
		    <include refid="PubLogWhere"></include>
		</where>
	</select>
	
	<select id="selectCountByDto"  parameterType="com.sdstc.system.model.PubLog" resultType="Integer">
		select
			count(*)
		from pub_log
		<where>
		    <include refid="PubLogWhere"></include>
		</where>
	</select>
	
	<select id="selectPageByDto" resultMap="PubLogMap">
		select
			<include refid="PubLogCols"></include>
		from pub_file
		<where>
		    <include refid="PubLogDtoWhere"></include>
		</where>
		limit #{pageDto.start},#{pageDto.pageSize}
	</select>
</mapper>