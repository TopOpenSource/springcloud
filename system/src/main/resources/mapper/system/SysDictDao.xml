<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.SysDictDao">
	
	<resultMap id="SysDictMap" type="com.sdstc.system.model.SysDict">
	    <id column="id" property="id" />
	    <result column="name" property="name" />
	    <result column="code" property="code" />
	    <result column="p_code" property="pCode" />
	    <result column="path" property="path" />
	    <result column="order_no" property="orderNo" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	</resultMap>
	
    <sql id="SysDictCols">
      id,name,code,p_code,path,order_no,gmt_create,gmt_modified,create_account,modified_account
    </sql>
    
    <sql id="SysDictProps">
      #{id},#{name},#{code},#{pCode},#{path},#{orderNo},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount}
    </sql>
    
    <sql id="SysDictWhere">
      
    </sql>
    
    <sql id="SysDictDtoWhere">
      
    </sql>
	
	<select id="selectByDto"  parameterType="com.sdstc.system.model.SysDict" resultMap="SysDictMap">
		select
			<include refid="SysDictCols"></include>
		from sys_dict
		<where>
		    <include refid="SysDictWhere"></include>
		</where>
	</select>
	
	<select id="selectCountByDto"  parameterType="com.sdstc.system.model.SysDict" resultType="Integer">
		select
			count(*)
		from sys_dict
		<where>
		    <include refid="SysDictWhere"></include>
		</where>
	</select>
	
	<select id="selectPageByDto" resultMap="SysDictMap">
		select
			<include refid="SysDictCols"></include>
		from pub_file
		<where>
		    <include refid="SysDictDtoWhere"></include>
		</where>
		limit #{pageDto.start},#{pageDto.pageSize}
	</select>
</mapper>