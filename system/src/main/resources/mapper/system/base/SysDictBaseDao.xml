<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.base.SysDictBaseDao">
	
	<resultMap id="SysDictMap" type="com.sdstc.system.model.SysDict">
	    <id column="id" property="id" />
	    <result column="name" property="name" />
	    <result column="code" property="code" />
	    <result column="p_code" property="pCode" />
	    <result column="path" property="path" />
	    <result column="order_no" property="orderNo" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	</resultMap>
	
    <sql id="SysDictCols">
      id,name,code,p_code,path,order_no,gmt_create,gmt_modified,create_account,modified_account
    </sql>
    
    <sql id="SysDictProps">
      #{id},#{name},#{code},#{pCode},#{path},#{orderNo},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount}
    </sql>
    
    <insert id="insert" parameterType="com.sdstc.system.model.SysDict">
        insert into sys_dict
        (<include refid="SysDictCols"></include>)
        values
        (<include refid="SysDictProps"></include>)
    </insert>
        
    <update id="updateSelectiveByPK" parameterType="com.sdstc.system.model.SysDict">
		update sys_dict
		<set>
		  
	        <if test="name != null and name != ''">
		        name = #{name},
		    </if>
		  
	        <if test="code != null and code != ''">
		        code = #{code},
		    </if>
		  
	        <if test="pCode != null and pCode != ''">
		        p_code = #{pCode},
		    </if>
		  
	        <if test="path != null and path != ''">
		        path = #{path},
		    </if>
		  
	        <if test="orderNo != null">
		        order_no = #{orderNo},
		    </if>
		  
	        <if test="gmtCreate != null">
		        gmt_create = #{gmtCreate},
		    </if>
		  
	        <if test="gmtModified != null">
		        gmt_modified = #{gmtModified},
		    </if>
		  
	        <if test="createAccount != null and createAccount != ''">
		        create_account = #{createAccount},
		    </if>
		  
	        <if test="modifiedAccount != null and modifiedAccount != ''">
		        modified_account = #{modifiedAccount},
		    </if>
		</set>
		<where>
		  id = #{id} 
		</where> 
	</update>
	
	<update id="updateByPK" parameterType="com.sdstc.system.model.SysDict">
		update sys_dict
		<set>
	    name = #{name},
	    code = #{code},
	    p_code = #{pCode},
	    path = #{path},
	    order_no = #{orderNo},
	    gmt_create = #{gmtCreate},
	    gmt_modified = #{gmtModified},
	    create_account = #{createAccount},
	    modified_account = #{modifiedAccount},
		</set>
		<where>
		  id = #{id} 
		</where> 
	</update>
	
	<delete id="deleteByPK">
		delete from sys_dict
		<where>
		  id = #{id} 
		</where> 
	</delete>
	
	<select id="selectByPK" resultMap="SysDictMap">
		select
			<include refid="SysDictCols"></include>
		from sys_dict
		<where>
		  id = #{id} 
		</where> 
	</select>
	
</mapper>