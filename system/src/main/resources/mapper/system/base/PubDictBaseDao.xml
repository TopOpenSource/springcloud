<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdstc.system.dao.base.PubDictBaseDao">
	
	<resultMap id="PubDictMap" type="com.sdstc.system.model.PubDict">
	    <id column="id" property="id" />
	    <result column="name" property="name" />
	    <result column="code" property="code" />
	    <result column="p_id" property="pId" />
	    <result column="path" property="path" />
	    <result column="gmt_create" property="gmtCreate" />
	    <result column="gmt_modified" property="gmtModified" />
	    <result column="create_account" property="createAccount" />
	    <result column="modified_account" property="modifiedAccount" />
	</resultMap>
	
    <sql id="PubDictCols">
      id,name,code,p_id,path,gmt_create,gmt_modified,create_account,modified_account
    </sql>
    
    <sql id="PubDictProps">
      #{id},#{name},#{code},#{pId},#{path},#{gmtCreate},#{gmtModified},#{createAccount},#{modifiedAccount}
    </sql>
    
    <insert id="insert" parameterType="com.sdstc.system.model.PubDict">
        insert into pub_dict
        (<include refid="PubDictCols"></include>)
        values
        (<include refid="PubDictProps"></include>)
    </insert>
        
    <update id="updateSelectiveByPK" parameterType="com.sdstc.system.model.PubDict">
		update pub_dict
		<set>
		  
	        <if test="name != null and name != ''">
		        name = #{name},
		    </if>
		  
	        <if test="code != null and code != ''">
		        code = #{code},
		    </if>
		  
	        <if test="pId != null">
		        p_id = #{pId},
		    </if>
		  
	        <if test="path != null and path != ''">
		        path = #{path},
		    </if>
		  
	        <if test="gmtCreate != null">
		        gmt_create = #{gmtCreate},
		    </if>
		  
	        <if test="gmtModified != null">
		        gmt_modified = #{gmtModified},
		    </if>
		  
	        <if test="createAccount != null and createAccount != ''">
		        create_account = #{createAccount},
		    </if>
		  
	        <if test="modifiedAccount != null and modifiedAccount != ''">
		        modified_account = #{modifiedAccount},
		    </if>
		</set>
		<where>
		  id = #{id} 
		</where> 
	</update>
	
	<update id="updateByPK" parameterType="com.sdstc.system.model.PubDict">
		update pub_dict
		<set>
	    name = #{name},
	    code = #{code},
	    p_id = #{pId},
	    path = #{path},
	    gmt_create = #{gmtCreate},
	    gmt_modified = #{gmtModified},
	    create_account = #{createAccount},
	    modified_account = #{modifiedAccount},
		</set>
		<where>
		  id = #{id} 
		</where> 
	</update>
	
	<delete id="deleteByPK">
		delete from pub_dict
		<where>
		  id = #{id} 
		</where> 
	</delete>
	
	<select id="selectByPK" resultMap="PubDictMap">
		select
			<include refid="PubDictCols"></include>
		from pub_dict
		<where>
		  id = #{id} 
		</where> 
	</select>
	
</mapper>